{
  "extends": "./node_modules/mwts/",
  "ignorePatterns": [
    "node_modules",
    "dist",
    "test",
    "jest.config.js",
    "typings"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "node/no-extraneous-require": [
      "error",
      {
        "allowModules": [],
        "resolvePaths": [],
        "tryExtensions": []
      }
    ]
  },
  "node/no-extraneous-import": [
    "error",
    {
      "allowModules": ["express"]
    }
  ]
}
